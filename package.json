{
  "name": "password-manager",
  "version": "1.0.0",
  "description": "个人密码管理器",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "build": "webpack --config webpack.config.js",
    "dev:renderer": "webpack-dev-server --config webpack.config.js --config-name renderer",
    "dev:main": "webpack --config webpack.config.js --config-name main && webpack --config webpack.config.js --config-name preload && electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && npm run dev:main\"",
    "build:prod": "webpack --config webpack.config.js --mode production && electron-builder --config electron-builder.json",
    "build:win": "webpack --config webpack.config.js --mode production && electron-builder --win --config electron-builder.json",
    "build:mac": "webpack --config webpack.config.js --mode production && electron-builder --mac --config electron-builder.json",
    "build:linux": "webpack --config webpack.config.js --mode production && electron-builder --linux --config electron-builder.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.3",
    "@types/node": "^18.15.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "canvas": "^3.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electron-webpack": "^2.8.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.4.0",
    "typescript": "^5.2.2",
    "wait-on": "^7.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  }
}
